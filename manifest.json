{
    "manifest_version": 2,
    "name": "Dog Pics Replace",
    "version": "0.1",
    "web_accessible_resources": [
      "Dog/*.jpeg"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js","sketch/p5.js"]
      }
    ],
    "permissions": ["tabs"],
    "browser_action": {
    
      "default_popup": "sketch/index.html",
      "default_title": "Generate a maze!"
    }
  }